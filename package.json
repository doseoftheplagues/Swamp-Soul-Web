{
  "name": "boilerplate-fullstack-routing",
  "type": "module",
  "version": "2.0.0",
  "main": "server/index.js",
  "license": "MIT",
  "keywords": [
    "fullstack",
    "routing"
  ],
  "scripts": {
    "test": "vitest",
    "start": "node dist/server.js",
    "dev": "run-p dev:client dev:server",
    "dev:client": "vite",
    "dev:server": "tsx watch server/index.ts",
    "build": "run-s build:client build:server",
    "build:client": "vite build",
    "build:server": "esbuild --packages=external --platform=node --format=esm --outfile=dist/server.js --bundle server/index.ts",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx .",
    "knex": "knex --knexfile ./server/db/knexfile.js",
    "render:build": "npm install && npm run build && npm prune --omit=dev && NODE_ENV='production' npm run knex migrate:latest",
    "render:start": "node dist/server.js"
  },
  "eslintConfig": {
    "extends": "@devacademy/eslint-config/react",
    "ignorePatterns": [
      "bundle.js"
    ]
  },
  "dependencies": {
    "@auth0/auth0-react": "^2.8.0",
    "@aws-sdk/client-s3": "^3.948.0",
    "@aws-sdk/lib-storage": "^3.948.0",
    "@gsap/react": "^2.1.2",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-form": "^0.1.8",
    "@radix-ui/react-icons": "^1.3.2",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-toast": "^1.2.15",
    "@tailwindcss/vite": "^4.1.16",
    "bootstrap": "^5.3.8",
    "bootstrap-icons": "^1.13.1",
    "dotenv": "^16.4.5",
    "express": "^4.17.2",
    "express-jwt": "^8.4.1",
    "gsap": "^3.14.2",
    "jsonwebtoken": "^9.0.2",
    "jwks-rsa": "^3.1.0",
    "knex": "^2.5.1",
    "multer": "^2.0.2",
    "pg": "^8.16.3",
    "radix-ui": "^1.4.3",
    "react-bootstrap": "^2.10.10",
    "react-bootstrap-icons": "^1.11.6",
    "react-date-picker": "^12.0.1",
    "react-router-dom": "^7.9.6",
    "sqlite3": "^5.1.7",
    "superagent": "^10.2.1",
    "tailwindcss": "^4.1.16"
  },
  "devDependencies": {
    "@devacademy/eslint-config": "^1.10.1",
    "@tanstack/eslint-plugin-query": "^5.12.2",
    "@tanstack/react-query": "^5.12.2",
    "@tanstack/react-query-devtools": "^5.13.3",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/express": "^4.17.14",
    "@types/gsap": "^1.20.2",
    "@types/multer": "^2.0.0",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@types/superagent": "^8.1.1",
    "@types/supertest": "^6.0.2",
    "@vitejs/plugin-react": "^4.2.1",
    "baseline-browser-mapping": "^2.9.11",
    "esbuild": "^0.18.19",
    "eslint": "^8.56.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.2",
    "jsdom": "^22.1.0",
    "nock": "^14.0.10",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.7.1",
    "react": "^18.0.2",
    "react-dom": "^18.2.0",
    "react-router": "^7.7.0",
    "sass": "^1.50.1",
    "supertest": "^6.3.4",
    "tsx": "^3.12.7",
    "typescript": "^5.3.3",
    "vite": "^5.0.0",
    "vitest": "^1.2.0"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "plugins": [
      "prettier-plugin-tailwindcss"
    ]
  },
  "browserslist": "> 2%, not dead"
}
